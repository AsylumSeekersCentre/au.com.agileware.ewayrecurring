    <url desc="Support">https://agileware.com.au/contact</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <license>AGPL</license>
  <maintainer>
    <author>Agileware</author>
    <email>support@agileware.com.au</email>
  </maintainer>
  <releaseDate>2019-05-09</releaseDate>
  <version>2.1.2-beta</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>4.7</ver>
    <ver>5.0</ver>
   </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/eWAYRecurring</namespace>
  </civix>
</extension>
